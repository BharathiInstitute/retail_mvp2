{
  "name": "retailpos-print-helper",
  "version": "1.0.0",
  "description": "Background print service for RetailPOS - enables one-click printing from web browsers",
  "type": "module",
  "main": "server.js",
  "license": "MIT",
  "bin": {
    "print-helper": "./server.js"
  },
  "pkg": {
    "scripts": "server.js",
    "assets": [],
    "targets": ["node18-win-x64"],
    "outputPath": "dist"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.2",
    "node-windows": "^1.0.0-beta.8",
    "pdf-to-printer": "^5.0.0",
    "pdfkit": "^0.15.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "scripts": {
    "start": "node server.js",
    "service:install": "node service.js install",
    "service:uninstall": "node service.js uninstall",
    "service:start": "node service.js start",
    "service:stop": "node service.js stop",
    "service:restart": "node service.js restart",
    "build": "pkg . --targets node18-win-x64 --output dist/PrintHelper.exe"
  }
}
